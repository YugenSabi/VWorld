{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ],
      "@/api": [
        "./api"
      ],
      "@/api/*": [
        "./api/*"
      ],
      "@/schemas": [
        "./schemas"
      ],
      "@/schemas/*": [
        "./schemas/*"
      ],
      "@/hooks": [
        "./hooks"
      ],
      "@/hooks/*": [
        "./hooks/*"
      ],
      "@/mocks": [
        "./mocks"
      ],
      "@/mocks/*": [
        "./mocks/*"
      ]
    },
    "jsx": "preserve"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
